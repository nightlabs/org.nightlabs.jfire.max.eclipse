            menubarPath="org.nightlabs.jfire.trade.ui.articleContainerAction"
            name="Attach an Issue"
            tooltip="Attach an Issue">
      </articleContainerAction>
   </extension>
   <extension
         point="org.nightlabs.jfire.issuetracking.ui.issueLinkHandlerFactory">
      <issueLinkHandlerCategory
            id="org.nightlabs.jfire.issuetracking.trade.ui.issueLinkHandlerCategory"
            name="Trade">
      </issueLinkHandlerCategory>
      <issueLinkHandlerFactory
            categoryId="org.nightlabs.jfire.issuetracking.trade.ui.issueLinkHandlerCategory"
            class="org.nightlabs.jfire.issuetracking.trade.ui.issuelink.IssueOfferLinkHandlerAdderFactory"
            name="Offer">
      </issueLinkHandlerFactory>
      <issueLinkHandlerFactory
            categoryId="org.nightlabs.jfire.issuetracking.trade.ui.issueLinkHandlerCategory"
            class="org.nightlabs.jfire.issuetracking.trade.ui.issuelink.IssueOrderLinkHandlerAdderFactory"
            name="Order">
      </issueLinkHandlerFactory>
      <issueLinkHandlerCategory
            id="org.nightlabs.jfire.issuetracking.trade.ui.accounting.issueLinkHandlerCategory"
            name="Accounting">
      </issueLinkHandlerCategory>
      <issueLinkHandlerFactory
            categoryId="org.nightlabs.jfire.issuetracking.trade.ui.accounting.issueLinkHandlerCategory"
            class="org.nightlabs.jfire.issuetracking.trade.ui.accounting.issuelink.IssueInvoiceLinkHandlerAdderFactory"
            name="Invoice">
      </issueLinkHandlerFactory>
      <issueLinkHandlerCategory
            id="org.nightlabs.jfire.issuetracking.trade.ui.store.issueLinkHandlerCategory"
            name="Store">
      </issueLinkHandlerCategory>
      <issueLinkHandlerFactory
            categoryId="org.nightlabs.jfire.issuetracking.trade.ui.store.issueLinkHandlerCategory"
            class="org.nightlabs.jfire.issuetracking.trade.ui.store.issuelink.IssueDeliveryNoteLinkHandlerAdderFactory"
            name="DeliveryNote">
       </issueLinkHandlerFactory>
       <issueLinkHandlerFactory
            categoryId="org.nightlabs.jfire.issuetracking.trade.ui.store.issueLinkHandlerCategory"
            class="org.nightlabs.jfire.issuetracking.trade.ui.store.issuelink.IssueReceptionNoteLinkHandlerAdderFactory"
            name="ReceptionNote">
      </issueLinkHandlerFactory>
   </extension>
   
  
