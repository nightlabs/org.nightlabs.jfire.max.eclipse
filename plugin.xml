<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension
         point="org.nightlabs.jfire.issuetracking.ui.issueOverview">
      	<categoryFactory
            categoryID="org.nightlabs.jfire.issuetracking.admin.ui.overview.issueproperty"
            class="org.nightlabs.jfire.issuetracking.admin.ui.overview.IssueOverviewCategoryFactory"
            index="1" name="Issue Types"/>
        <entryFactory
            categoryEntryID="org.nightlabs.jfire.pissuetracking.admin.ui.overview.issuetype.IssueTypeEntry"
            categoryID="org.nightlabs.jfire.issuetracking.admin.ui.overview.issueproperty"
            class="org.nightlabs.jfire.issuetracking.admin.ui.overview.IssueTypeEntryFactory"
            index="0" name="Issue Types"/>
    </extension>
    
    <extension
         point="org.eclipse.ui.editors">
      <editor
        	class="org.nightlabs.jfire.issuetracking.admin.ui.overview.issueproperty.IssueTypeNewEditor"
            default="false"
            contributorClass="org.nightlabs.base.ui.action.registry.editor.XEditorActionBarContributor"
            icon="icons/articlecontainer/detail/deliverynote/GeneralEditorInputDeliveryNote.16x16.png"
            id="org.nightlabs.jfire.issuetracking.admin.ui.overview.issueproperty.IssueTypeNewEditor"
            name="Issue Type">
      </editor>
   </extension>
   
    <extension point="org.nightlabs.base.ui.entityEditor">
		<pageFactory
			class="org.nightlabs.jfire.issuetracking.admin.ui.overview.IssueOverviewPage$Factory"
			editorID="org.nightlabs.jfire.issuetracking.admin.ui.overview.IssueEditor"
			indexHint="10" />
		<pageFactory
			class="org.nightlabs.jfire.issuetracking.admin.ui.overview.issueproperty.IssueTypeNewEditorPage$Factory"
			editorID="org.nightlabs.jfire.issuetracking.admin.ui.overview.issueproperty.IssueTypeNewEditor"
			indexHint="10" />
  </extension>  
  
